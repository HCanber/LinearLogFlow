{
    "dynamic_templates": [
        {
            "string_properties_fields": {
                "path_match": "properties.*",
                "match_mapping_type": "string",
                "mapping": {
                    "{name}": {
                        "type": "string",
                        "index": "analyzed",
                        "norms": {
                            "enabled": false
                        },
                        "index_options": "docs",

                        "fields": {
                            "raw": {
                                "index": "not_analyzed",
                                "ignore_above": 256,
                                "type": "string"
                            }
                        }

                    }
                }
            }
        }
    ],
    "properties": {
        "@timestamp": {
            "index": "not_analyzed",
            "type": "date"
        },
        "level": {
            "index": "not_analyzed",
            "type": "string"
        },
        "message": {
            "index": "analyzed",
            "type": "string"
        },
        "messageHash": {
            "index": "not_analyzed",
            "type": "string"
        },
        "exception": {
            "dynamic": true,
            "path": "full",
            "type": "object"
        },
        "exceptionHash": {
            "index": "not_analyzed",
            "type": "string"
        },
        "template": {
            "index": "analyzed",
            "type": "string"
        },
        "templateHash": {
            "index": "not_analyzed",
            "type": "string"
        }
    },
    "_all": {
        "enabled": false
    }
}
